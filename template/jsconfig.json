{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~background/*": [
                "src/background/*"
            ],
            "~contentScript/*": [
                "src/contentScript/*"
            ],
            "~options/*": [
                "src/options/*"
            ],
            "~popup/*": [
                "src/popup/*"
            ],
            "~styles/*": [
                "src/styles/*"
            ],
            "~/*": [
                "src/*"
            ]
        }
    }
}